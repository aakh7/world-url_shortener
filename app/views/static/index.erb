<form action="/form" method="post">
	Enter URL:
	<input type ="text" name="long_url">
	<input type ="submit" name="Submit">
</form>

<table style="width:100%">
	<tr>
		<th>Long URL</th>
		<th>Shortened URL</th>
	</tr>
	<% Url.all.each do |url| %>
	<tr>
		<td><a href="<%=url.shortened_url%>"><%= url.long_url%></a></td>
		<td><a href="<%=url.shortened_url%>"><%=url.shortened_url%> </a></td>
	</tr>
<% end %>
</table>



